{
  "name": "neovim-ink",
  "version": "0.1.0",
  "description": "Embeddable Neovim editor component for Ink (React for the terminal)",
  "type": "module",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "bin": {
    "neovim-ink": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.tsx",
    "dev": "tsx watch src/index.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "neovim",
    "nvim",
    "ink",
    "react",
    "tui",
    "terminal",
    "editor",
    "embeddable"
  ],
  "author": "ohwang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohwang/neovim-ink.git"
  },
  "homepage": "https://github.com/ohwang/neovim-ink#readme",
  "bugs": {
    "url": "https://github.com/ohwang/neovim-ink/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "ink": ">=5.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "neovim": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "ink": "^6.8.0",
    "react": "^19.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
